<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_auth"
    app:startDestination="@id/PINCodeFragment">

    <fragment
        android:id="@+id/chooseRoleFragment2"
        android:name="fi.dvv.digiid.poc.wallet.ui.auth.ChooseRoleFragment"
        android:label="ChooseRoleFragment"
        tools:layout="@layout/fragment_choose_role">
        <action
            android:id="@+id/toPINCodeEntry"
            app:destination="@id/PINCodeFragment" />
    </fragment>
    <fragment
        android:id="@+id/PINCodeFragment"
        android:name="fi.dvv.digiid.poc.wallet.ui.auth.PINCodeFragment"
        android:label="PINCodeFragment"
        tools:layout="@layout/fragment_pin_code">
        <action
            android:id="@+id/toMainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="fi.dvv.digiid.poc.wallet.ui.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>